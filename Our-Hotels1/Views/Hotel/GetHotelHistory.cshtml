@model IEnumerable<HotelHistoryDto>
@*@model IEnumerable<GroupedHotelReviewDto>*@
@{
	Layout = "_HotelLayout";
	ViewData["title"]= " Visitor bill Price";
}

   
					
								
							
						
							
							<div class="row">
								<div class="col-md-12">
								@*	<h4 class="mb-4">Patient Appoinment</h4>*@
									<div class="appointment-tab">
									
										<!-- Appointment Tab -->
										<ul class="nav nav-tabs nav-tabs-solid nav-tabs-rounded">
											<li class="nav-item">
												<a class="nav-link active" href="#upcoming-appointments" data-toggle="tab">Visitors Paid</a>
											</li>
										</ul>
										<!-- /Appointment Tab -->
										
										<div class="tab-content">
										
											<!-- Upcoming Appointment Tab -->
											<div class="tab-pane show active" id="upcoming-appointments">
												<div class="card card-table mb-0">
													<div class="card-body">
														<div class="table-responsive">
															<table class="table table-hover table-center mb-0">
																<thead>
																	<tr>
																		<th>Visistor Name</th>
																		<th>Visistor Mobile</th>
																		<th>Booking Date</th>
																		<th class="text-center">Paid Amount</th>
																	</tr>
																</thead>
	
															<tbody>
															
																	
																	@foreach( var item in Model)
											{
													<tr>
																		
																		<td>@item.CustomerFirstName <span> </span> @item.CustomerlastName</td>
																		<td>@item.CustomerMobile</td>
																		<td>@item.BillDate</td>
																	   <td>@item.TotalPrice</td>
																		
																	</tr>
											}
																</tbody>
															</table>		
														</div>
													</div>
												</div>
											</div>
											<!-- /Upcoming Appointment Tab -->
									   </div>
									</div>
								</div>
							</div>

				
						